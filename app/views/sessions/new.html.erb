<p id="notice"><%= notice %> </p>

<h1>Log In</h1>

<%= form_with model: @session, url: '/sessions' do |f|%>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="actions"><%= f.submit "Log In" %></div>
<% end %>